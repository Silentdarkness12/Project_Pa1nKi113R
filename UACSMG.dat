		ATFR GGGGGGGGGHIK 1 A_JumpIfInventory("Kicking",1,"DoKick")
		ATFR L 1 A_PlaySound("RIFCL_IN", 1)
		ATFR M 5 A_JumpIfInventory("Kicking",1,"DoKick")
		ATFR NOPQRS 1 A_JumpIfInventory("Kicking",1,"DoKick")

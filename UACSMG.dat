		ATFF A 1 BRIGHT A_AlertMonsters
		TNT1 A 0 A_FireCustomMissile("YellowFlareSpawn",0,0,0,0)
		TNT1 A 0 A_SetAngle(angle - random(-.15,1.05))
		TNT1 A 0 A_SetPitch(pitch - 0.33)

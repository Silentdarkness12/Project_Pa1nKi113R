Actor Trooperbullet: FastProjectile
{
	Projectile
	+RANDOMIZE
	+FORCEXYBILLBOARD
   +BLOODSPLATTER 
   +NOEXTREMEDEATH
   -TELESTOMP
  +NOTELEFRAG
damage (random(3,5))
damagetype fire
DamageFactor "Blood", 0.0 DamageFactor "BlueBlood", 0.0 DamageFactor "GreenBlood", 0.0
radius 2
height 2
renderstyle ADD
alpha 0.9
speed 140
scale .15
Decal "Scorch"
states
	{
	Spawn:
		TRAC A 1 BRIGHT
		Loop
Xdeath:
   
	TNT1 A 0 A_CustomMissile ("Flametrails", 0, 0, random (0, 180), 2, random (0, 360))
	TNT1 B 4
	TNT2 AAA 4 A_CustomMissile ("PlasmaSmoke", 1, 0, random (0, 360), 2, random (0, 160))
		
		Stop

	Death:

	TNT1 A 0 A_CustomMissile ("Flametrails", 0, 0, random (0, 180), 2, random (0, 360))
	TNT1 B 4	
    TNT2 AAAAAAAAA 4 A_CustomMissile ("PlasmaSmoke", 1, 0, random (0, 360), 2, random (0, 160))
		Stop
	}
}

Actor TrooperSoul: Homingsoul 
{
  Health 22
  PROJECTILE
  +RANDOMIZE
  +SEEKERMISSILE
 +DONTSEEKINVISIBLE
 +BLOODSPLATTER 
 +FORCEXYBILLBOARD
 Decal "Scorch"
  damage 1
  radius 16
  height 16
  speed 12
  damagetype Fire
  renderstyle Add
  alpha 0.6
  states
  {
  Spawn:
	FX15 A 2 Bright
    FX15 B 2 Bright A_CHolySeek
    FX15 C 2 Bright A_Seekermissile(90,90)
    FX15 B 2 Bright A_CHolyCheckScream
    loop

   Death:
	TNT1 A 0 A_Playsound("HRSteam")
	FX00 JKLM 3
    TNT1 AAAAAAA 0 A_CustomMissile ("Flametrails", 0, 0, random (0, 180), 2, random (0, 360))
	TNT1 B 4	
    TNT2 AAAAAAAAA 4 A_CustomMissile ("PlasmaSmoke", 1, 0, random (0, 360), 2, random (0, 160))
	
	stop
	 XDeath:
	 
	 TNT1 A 0 A_Playsound("HRSteam")
	 FX00 JKLM 3
    TNT1 AAAAAAA 0 A_CustomMissile ("Flametrails", 0, 0, random (0, 180), 2, random (0, 360))
	TNT1 B 4
    TNT2 AAAAAAAAA 4 A_CustomMissile ("PlasmaSmoke", 1, 0, random (0, 360), 2, random (0, 160))
	
	stop
   }
}



ACTOR HellTrooper 2848
{
	//$Category Monsters
	Game Doom
	Health 220
	Radius 16
	Height 44
    Speed 9
	FastSpeed 14
	PainChance 150
	PainChance "Avoid", 100
	PainChance "Kick", 200
	PainChance "Melee", 150
	PainChance "Taunt", 0
	damagefactor "Fatality", 0.5
	damagefactor "Taunt", 0.0
	damagefactor "SSG", 1.5
	damagefactor "HelperMarineFatallity", 3.0
	DamageFactor "Blood", 0.0 DamageFactor "BlueBlood", 0.0 DamageFactor "GreenBlood", 0.0
	DamageFactor "bloodsplasher", 0.0    DamageFactor "tinybloodsplasher", 0.0    DamageFactor "superbloodsplasher", 0.0    DamageFactor "smallbloodsplasher", 0.0
	DamageFactor "BlueBloodSplasher", 0.0
    DamageFactor "BlueSuperBloodSplasher", 0.0
	damagefactor "SuperPunch", 3.0
	damagefactor "Melee", 3.0
	damagefactor "Freeze", 2.0
	PainChance "ExplosiveImpact", 200
	Monster
	+FLOORCLIP
    +MISSILEMORE
	//-TELESTOMP
	-NOTELEFRAG
	+NOTELESTOMP
	+TELESTOMP
    Scale 1.1
	MaxStepHeight 24
	MaxDropOffHeight 32
    BloodType "NashGore_Blood"
	SeeSound "grunt/sight"
	PainSound "grunt/pain"
	DeathSound "grunt/death"
	ActiveSound "grunt/active"
    damagefactor "Blood", 0.0 damagefactor "BlueBlood", 0.0 damagefactor "GreenBlood", 0.0
    damagefactor "Avoid", 1.1
    damagefactor "Kick", 0.3
    damagefactor "MinorHead", 2.0
	damagefactor "Head", 2.0
	damagefactor "Shrapnel", 0.4
	damagefactor "ExplosiveImpact", 2.0
	Obituary "%o has been purged by a hell trooper."
	damagefactor "Taunt", 1.0
	DropItem "CombatKnife", 2
//	DropItem "Hell_rifle" // needs to be seperated!!!
	States
	{
    Spawn:
	HTWA C 1
	Goto Stand
	
	Stand:
		HTWA A 0 A_CheckSight("Stand2")
        TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 5, 40,0)
		HTWA C 10 A_Look
		Loop
	Stand2:
		HTWA C 1 A_Look
		Goto Spawn
	See:
        TNT1 A 0 A_SpawnItemEx("FootStep", 0, 0, 10, 0, 0)
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 40,0)
		HTWA AA 2 A_Chase
		TNT1 A 0 A_Recoil(-1)
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 40,0)
		HTWA AA 2 A_Chase
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 40,0)
		HTWA BB 2 A_Chase
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 40,0)
		HTWA BB 2 A_Chase
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 40,0)
		HTWA CC 2 A_Chase
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 40,0)
		HTWA CC 2 A_Chase
		
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 40,0)
		HTWA DD 2 A_Chase
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 5, 40,0)
		HTWA DD 2 A_Chase
		
		
		Loop
		
		Pain.Taunt:
		TNT1 A 0
		TNT1 A 0 A_JumpIfInventory("Enraged", 1, "SpecialAttack")
		HTWA B 0 A_Pain
		TNT1 A 0 A_ChangeFlag("FASTER", 1)
		TNT1 A 0 A_ChangeFlag("MISSILEMORE", 1)
		TNT1 A 0 A_ChangeFlag("MISSILEEVENMORE", 1)
		TNT1 A 0 A_PlaySound("grunt/sight")
		HTWA A 1 A_FaceTarget
		HTWA A 1
		TNT1 A 0 A_GiveInventory("Enraged", 1)
		TNT1 A 0 A_PlaySound("grunt/sight", 1)
		Goto SpecialAttack
		
Missile: 
		TNT1 A 0 A_JumpIfInventory("Enraged", 1, "SpecialAttack")
        TNT1 A 0 A_Jump(32,"SpecialAttack")
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 40,0)
		TNT1 A 0 A_SpawnItem ("LegTarget12", 10)
		HTF1 A 6 A_FaceTarget
		TNT1 A 0 A_PlaySound("HRFire")
		TNT1 A 0 A_CustomMissile("Trooperbullet", 35, 0, random(-8, 8), 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 40,0)
		TNT1 A 0 A_SpawnItem ("LegTarget12", 10)
		HTF1 B 6 A_FaceTarget
		TNT1 A 0 A_CPosRefire
		Goto See
		
SpecialAttack:	//more shots (rapidfire!!!)	
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 40,0)
		TNT1 A 0 A_SpawnItem ("LegTarget12", 10)
		HTF1 A 3 A_FaceTarget
		TNT1 A 0 A_PlaySound("HRFire")
		TNT1 A 0 A_CustomMissile("Trooperbullet", 35, 0, random(-8, 8), 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 40,0)
		TNT1 A 0 A_SpawnItem ("LegTarget12", 10)
		HTF1 B 3 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 40,0)
		TNT1 A 0 A_SpawnItem ("LegTarget12", 10)
		HTF1 A 3 A_FaceTarget
		TNT1 A 0 A_PlaySound("HRFire")
		TNT1 A 0 A_CustomMissile("Trooperbullet", 35, 0, random(-8, 8), 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 40,0)
		TNT1 A 0 A_SpawnItem ("LegTarget12", 10)
		HTF1 B 3 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 40,0)
		TNT1 A 0 A_SpawnItem ("LegTarget12", 10)
		HTF1 A 3 A_FaceTarget
		TNT1 A 0 A_PlaySound("HRFire")
		TNT1 A 0 A_CustomMissile("Trooperbullet", 35, 0, random(-8, 8), 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 40,0)
		TNT1 A 0 A_SpawnItem ("LegTarget12", 10)
		HTF1 B 3 A_FaceTarget
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 40,0)
		TNT1 A 0 A_SpawnItem ("LegTarget12", 10)
		HTF1 A 3 A_FaceTarget
		TNT1 A 0 A_PlaySound("HRFire")
		TNT1 A 0 A_CustomMissile("Trooperbullet", 35, 0, random(-8, 8), 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 40,0)
		TNT1 A 0 A_SpawnItem ("LegTarget12", 10)
		HTF1 B 3 A_FaceTarget
		TNT1 A 0 A_CPosRefire
        Goto Missile

    

    Pain.KillMe:
        TNT1 A 0
        Goto See
		
    Death.KillMe:
    Death.Taunt:
    Death.Avoid:
	    TNT1 A 0
		TNT1 A 0 A_ChangeFlag("SOLID", 0)
        TNT1 A 0 A_SpawnItem("HellTrooper")
		TNT1 A 0
        Stop

	Pain:
        TNT1 A 0 A_JumpIfInventory ("IsDown", 1, 7)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 0, 40,0)
		TNT1 A 0 A_CustomMissile ("Nashgore_FlyingBlood", 35, 0, random (0, 360), 2, random (0, 160))
	    HTPA A 3
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 0, 40,0)
		HTPA A 3 A_Pain
		Goto See
        TNT1 AAAA 0
		HTD1 G 4
        TNT1 A 0 A_Pain
        HT11 SSST 10
        TNT1 A 0 A_TakeInventory("IsDown", 1)
        Goto See

        Pain.Melee:
        TNT1 A 0 A_JumpIfInventory ("IsDown", 1, 7)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 0, 40,0)
		TNT1 A 0 A_CustomMissile ("Nashgore_FlyingBlood", 35, 0, random (0, 360), 2, random (0, 160))
	    HTPA A 3
		TNT1 A 0 A_SpawnItem ("HeadshotTarget4", 0, 40,0)
		HTPA A 3 A_Pain
		Goto See
        TNT1 AAAA 0
		HTD1 G 4
        TNT1 A 0 A_Pain
        HT11 SSST 10
        TNT1 A 0 A_TakeInventory("IsDown", 1)
        Goto See

		
	Pain.ExplosiveImpact:
	Pain.Explosive:
	         TNT1 A 0
	         TNT1 A 0 A_Pain
		 TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Pain")
		 		    TNT1 A 0 A_TakeInventory("SKHellTrooper", 1)
         TNT1 A 0 A_GiveInventory("IsDown", 1)
        TNT1 A 0 ThrustThingZ(0,40,0,1)
         HT11 GH 8
		Goto FallingAfterImpact
		// Falling back animation
    Pain.Kick:
	Pain.BlackHole:
         TNT1 A 0
         HT11 G 1 A_Pain
		 TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Pain")
		  TNT1 A 0 A_TakeInventory("SKHellTrooper", 1)
         TNT1 A 0 A_GiveInventory("IsDown", 1)
        TNT1 A 0 ThrustThingZ(0,30,0,1)
		//ID11 E 1 A_Chase ("","")
		HT11 G 1 A_facetarget
        HT11 G 6 ThrustThing(angle*256/360+128, 10, 0, 0)
        HT11 H 8
		Goto FallingAfterImpact
		
		FallingAfterImpact:
		HT11 H 1 A_GiveInventory ("Fallingheight2", 1)
		HT11 H 1 A_CheckFloor ("GetUp")
		NULL A 0 A_JumpIf (momz == 0, "GetUp")
		Loop
		
		GetUp:
		TNT1 A 0
		TNT1 A 0 A_JumpIfInventory ("Fallingheight2", 20, "Death.Stomp")
		TNT1 A 0 A_JumpIfInventory ("Fallingheight2", 14, "FallingSuperDeath")
		TNT1 A 0 A_JumpIfInventory ("Fallingheight2", 7, "FallingDeath")
		HT11 SSSSSSS 5 A_JumpIf(momz < 0, "FallingAfterImpact")
		TNT1 A 0 A_TakeInventory("Fallingheight2", 100)
        TNT1 A 0 A_TakeInventory("IsDown", 1)
		HT11 T 10
         Goto See
		 
		
	FallingDeath:
	TNT1 A 0
	TNT1 A 0 A_Explode(200, 2, 1)
	TNT1 A 0 A_Scream
	TNT1 A 0 A_NoBLocking
	TNT1 AAAA 0 A_CustomMissile ("XDeath1", 32, 0, random (0, 360), 2, random (50, 130))
	HT11 GH 2
	TNT1 A 0 A_SpawnItem ("BigBloodSpot")
	TNT1 A 0 A_SpawnItem ("Hellgun")
	HTMS L -1
	Stop
	
	FallingSuperDeath:
	TNT1 A 0
	TNT1 A 0 A_Explode(200, 2, 1)
	TNT1 A 0 A_XScream
	TNT1 A 0 A_NoBLocking
	TNT1 A 0 A_SpawnItem ("BigBloodSpot")
	TNT1 A 0 A_SpawnItem ("Hellgun")
	Goto Death.ExplosiveImpact+1
	

	    Death.Melee:
        TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Death.Down")
		TNT1 A 0 A_JumpIfInventory ("SKHellTrooper", 1, "StealthKill")
        Death.Kick:
		TNT1 A 0 ThrustThingZ(0,30,0,1)
		TNT1 A 0 A_Scream
		TNT1 A 0 A_NoBlocking
        TNT1 A 0 A_FaceTarget
        TNT1 A 0 A_Recoil (8)
		TNT1 A 0 A_SpawnItem ("Hellgun")
         HT11 GH 8
		 HTMS L -1
		Stop

		 

   Death.Down:
		POSS I 0 A_Scream
		POSS J 0 A_NoBlocking
		HTMS L 1
        HTMS L -1
        Stop


Death.Shotgun:
		TNT1 A 0 A_JumpIfCloser(200, "Death.Shotgunontheface")
		Goto Death
		
Death.Shotgunontheface:	
	    
		TNT1 A 0
		TNT1 A 0 A_Jump(128, "Death.Arm", "Death.Leg")
	    TNT1 A 0 ThrustThingZ(0,40,0,1)
		TNT1 A 0 A_CustomMissile ("MuchBlood2", 35, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_Scream
		TNT1 A 0 A_NoBlocking
         HT11 GH 8
         HTMS L -1
		 Stop
		 
 StealthKill:
	TNT1 A 1
	TNT1 A 0 A_GiveToTarget("GoFatality", 1)
	TNT1 A 0 A_GiveToTarget("SKHelltrooper", 1)
	TNT1 A 0 A_NoBlocking
	TNT1 A 0 A_Die("Stealthy")
	TNT1 A 0 A_Fall
	TNT1 A 1
	Stop
	
	Death.Stealthy:
	TNT1 A 0
	Stop

	Death:
        TNT1 A 0 A_JumpIfInventory ("IsDown", 1, "Death.Down")
        TNT1 A 0
		TNT1 AA 0 A_CustomMissile ("Nashgore_FlyingBlood", 7, 0, random (0, 360), 2, random (0, 160))
        TNT1 A 0 A_Jump(96, "Death.Arm", "Death.Leg")
		TNT1 A 0 A_Jump(128, "DeathMirror","Death.Vanish")
		TNT1 A 0 A_SpawnItem ("Hellgun")
		HTMS H 5
		HTMS I 5 A_Scream
		HTMS J 5 A_NoBlocking
		HTMS K 5
		//TNT1 A 0 A_SpawnItem ("MediumBloodSpot")
        HTMS L -1
		Stop
	
	DeathMirror:
	TNT1 A 0 A_SpawnItem ("Hellgun")
	    HTMS M 5
		HTMS N 5 A_Scream
		HTMS O 5 A_NoBlocking
		HTMS P 5
		TNT1 A 0 A_SpawnItem ("MediumBloodSpot")
        HTMS Q -1
		Stop
	
// own addition
	Death.Vanish:
	Death.BlackHole:
	TNT1 A 0 A_SpawnItem ("Hellgun")
	HTD6 A 5
	HTD6 B 5 A_Scream
	HTD6 C 5 A_NoBlocking
	HTD6 DEFG 5
	Stop
//	
     Death.Arm:

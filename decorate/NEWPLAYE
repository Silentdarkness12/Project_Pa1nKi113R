//ArachnoGunner
ArachnoGunner:
	TNT1 A 0 SetPlayerProperty(0,1,0)
 //   TNT1 A 0 ACS_Execute(477,0,0,0,0)
//	TNT1 A 0 ACS_Execute(acsFatality, 0)
	TNT1 A 0 A_JumpIfInventory("ArachnotronTurretFiring", 1, "ArachnoGunFire")
	ARCT A 3  A_JumpIfInventory("ArachnotronTurretFiring", 1, "ArachnoGunFire")
	TNT1 A 0 A_JumpIfInventory("ArachnotronTurretFiring", 1, "ArachnoGunFire")
	TNT1 A 0 A_JumpIfInventory("ArachnotronTurret", 1, "ArachnoGunner")
	TNT1 A 0 A_SpawnItemEx("NoLegsSpider", 0, 1, 1, 1, 1)
	TNT1 A 0 A_TakeInventory("GoFatality", 1)
	TNT1 A 0 SetPlayerProperty(0,0,0)
	Goto See
	
ArachnoGunnerPain:
	TNT1 A 0 SetPlayerProperty(0,1,0)
 //   TNT1 A 0 ACS_Execute(477,0,0,0,0)
//	TNT1 A 0 ACS_Execute(acsFatality, 0)
	ARCT A 3  A_JumpIfInventory("ArachnotronTurretFiring", 1, "ArachnoGunFire")
	//TNT1 A 0 A_JumpIfInventory("ArachnotronTurretFiring", 1, "ArachnoGunFire")
	TNT1 A 0 A_JumpIfInventory("ArachnotronTurret", 1, "ArachnoGunner")
	TNT1 A 0 A_SpawnItemEx("NoLegsSpider", 0, 1, 1, 1, 1)
	TNT1 A 0 A_TakeInventory("GoFatality", 1)
	TNT1 A 0 SetPlayerProperty(0,0,0)
	Goto See
	

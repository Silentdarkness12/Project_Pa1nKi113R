ACTOR HeadshotTargetBase
{
	+NOTARGET	+NEVERTARGET
	DamageFactor "Cut", 5.0
	DamageFactor "SSG", 9.0
	DamageFactor "Melee", 0.3
	DamageFactor "Cutless", 1.0
	DamageFactor "Plasma", 5.9
	DamageFactor "Plasma2", 0.0
	DamageFactor "Blast", 3.0
	DamageFactor "Taunt", 0.0
	DamageFactor "Blood", 0.0
	DamageFactor "BlueBlood", 0.0
	DamageFactor "Head", 0.0
	DamageFactor "MinorHead", 0.0
	DamageFactor "KillMe", 0.0
	DamageFactor "Explosive", 2.0
	DamageFactor "Kick", 2.0
	DamageFactor "Fire", 5.0
	DamageFactor "Burn", 5.0
	DamageFactor "Flames", 5.0
	DamageFactor "Control", 0.0
	DamageFactor "Shotgun", 0.7
	Monster
	-COUNTKILL
	Health 35
	Radius 7
	Height 18
	DamageType head
	BloodType "NashGore_Blood"
	DamageFactor "Reloading", 0.0
	PainChance 255
	+NOGRAVITY
	+SYNCHRONIZED
//	+SHOOTABLE
	+NOTELEPORT
	-SOLID
	+NODAMAGETHRUST
	+THRUGHOST
	+NORADIUSDMG
	+GHOST
	+NOBLOODDECALS
	+SERVERSIDEONLY
//	-COUNTKILL
	DamageFactor "Trample", 0
	States
	{
	Spawn:
		TNT1 A 0
		TNT1 A 0 A_CheckSight("Disappear")
		XXXX A 5
		XXXX A 1
		Stop
	Pain:
		TNT1 A 4
		TNT1 A 0// A_PlaySound("fleshimpact", 2)
		TNT1 A 0 A_SpawnItemEx ("HeadshotMinorHit")
		TNT1 A 1
		Stop
	Pain.Explosive:
	Pain.ExplosiveImpact:
		TNT1 A 4
		TNT1 A 0 A_SpawnItemEx ("HeadshotMinorHit")
		TNT1 A 1
		Stop
	Pain.CutLess:
		TNT1 A 4
		TNT1 A 1 A_SpawnItemEx ("HeadshotMinorHitChaingun", 0, 0)
		TNT1 A 1
		Stop

	Pain.AK:
		TNT1 A 4
		TNT1 A 0 A_PlaySound("fleshimpact", 2)
		TNT1 A 0 A_Jump(128,"PainAK2")
		TNT1 A 1 A_SpawnItem ("HeadshotMinorHit", 0, 0)
		TNT1 A 1
		Stop
	PainAK2:
		TNT1 A 0 A_PlaySound("fleshimpact", 2)
		TNT1 A 1 A_SpawnItem ("HeadshotHit", 0, 0)
		TNT1 A 1
		Stop

	Death:
		TNT1 A 0// A_PlaySound("HEDSHOT", 1)
		TNT1 A 1 A_SpawnItem ("HeadshotHit", 0, 0)
		TNT1 A 1
		Stop
	Death.Plasma:
	Death.Plasma2:
	Death.BigPlasma:
		TNT1 A 1 A_SpawnItem ("HeadshotHitPlasma", 0, 0)
		TNT1 A 1
		Stop

	Death.Flames:
	Death.Fire:
	Death.Burn:
		TNT1 A 1 A_SpawnItem ("HeadshotHitFire", 0, 0)
		TNT1 A 1
		Stop

	Death.Cut:
		TNT1 A 1 A_SpawnItem ("HeadshotHitSaw", 0, 0)
		TNT1 A 1
		Stop
	Death.Explosive:
	XDeath:
		TNT1 A 0// A_PlaySound("HEDEXPL", 1)
		TNT1 A 1 A_SpawnItem ("HeadshotX", 0, 0)
		TNT1 A 1
		Stop
	Pain.SSG:
	Death.SSG:
		TNT1 A 0// A_PlaySound("HEDEXPL", 1)
		TNT1 A 1 A_SpawnItem ("HeadshotSSG", 0, 0)
		TNT1 A 1
		Stop
	Death.Taunt:
		TNT1 A 1 A_SpawnItem ("HeadshotHitTaunt", 0, 0)
		TNT1 A 1
		Stop

	Disappear:
		TNT1 A 0
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		Stop
	}
}


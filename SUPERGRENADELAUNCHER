	   TNT1 A 0 A_PlaySound ("barlp1",3)
	   TNT1 A 2 Bright A_SpawnItem("GreenFlare",0,0)
	   BXPL A 0 Bright A_Explode(14*random(2.25,2.5),128, 0) 
	  TNT1 AA 0 A_SpawnItem("ACIDFOG", 0, 0)
	  TNT1 A 0 A_SpawnItemEx("PlasmaParticleSpawner", 0, 0, 0, 6, 6, 6, 0, 128)
	    TNT1 A 0 A_SpawnItemEx("PlasmaParticleSpawner", 0, 0, 0, 6, 6, 6, 0, 128)
	   TNT1 AA 0 A_SpawnItem("ACIDFOG", 0, 0)
	  TNT1 A 0 A_SpawnItemEx("PlasmaParticleSpawner", 0, 0, 0, 6, 6, 6, 0, 128)
	   TNT1 A 0 A_SpawnItem("BFGAltShockWave",0,0)
	   TNT1 A 19 A_CustomMissile("PlasmaSmoke", 1, 0, random (0, 360), 2, random (0, 160))
	   Stop
	   
	   XDeath:
	   TNT1 A 0 A_SpawnItem ("BigRicoChet", 0, -30)
		TNT1 A 0 A_StopSound(6)
	   TNT1 A 0 A_PlaySound ("barlp1",3)
	   TNT1 A 2 Bright A_SpawnItem("GreenFlare",0,0)
	   BXPL A 0 Bright A_Explode(14*random(2,3),128, 0) 
	  TNT1 AA 0 A_SpawnItem("ACIDFOG", 0, 0)
	  TNT1 A 0 A_SpawnItemEx("PlasmaParticleSpawner", 0, 0, 0, 6, 6, 6, 0, 128)
	    TNT1 A 0 A_SpawnItemEx("PlasmaParticleSpawner", 0, 0, 0, 6, 6, 6, 0, 128)
	   TNT1 AA 0 A_SpawnItem("ACIDFOG", 0, 0)
	  TNT1 A 0 A_SpawnItemEx("PlasmaParticleSpawner", 0, 0, 0, 6, 6, 6, 0, 128)
	   TNT1 A 0 A_SpawnItem("BFGAltShockWave",0,0)
	   TNT1 A 19 A_CustomMissile("PlasmaSmoke", 1, 0, random (0, 360), 2, random (0, 160))
	   Stop
	Crash:
	   TNT1 A 0 A_StopSound(6)
	   TNT1 A 0 A_PlaySound ("barlp1",3)
	   TNT1 A 2 Bright A_SpawnItem("GreenFlare",0,0)
	   BXPL A 0 Bright A_Explode(14*random(2,3),128, 0) 
	  TNT1 AA 0 A_SpawnItem("ACIDFOG", 0, 0)
	  TNT1 A 0 A_SpawnItemEx("PlasmaParticleSpawner", 0, 0, 0, 6, 6, 6, 0, 128)
	    TNT1 A 0 A_SpawnItemEx("PlasmaParticleSpawner", 0, 0, 0, 6, 6, 6, 0, 128)
	   TNT1 AA 0 A_SpawnItem("ACIDFOG", 0, 0)
	  TNT1 A 0 A_SpawnItemEx("PlasmaParticleSpawner", 0, 0, 0, 6, 6, 6, 0, 128)
	   TNT1 A 0 A_SpawnItem("BFGAltShockWave",0,0)
	   TNT1 A 19 A_CustomMissile("PlasmaSmoke", 1, 0, random (0, 360), 2, random (0, 160))
	   Stop
	}
	
}

ACTOR ACIDFOG
{
    Radius 20
    Height 12
	Alpha .8
	Decal "Scorch"
	RenderStyle Translucent
	Damage 1
	DamageType "Desintegrate"
	Scale 1.2
	Speed 0
	Gravity 0
    +NOBLOCKMAP
    +NOTELEPORT
	+NOEXTREMEDEATH
	-EXTREMEDEATH
	+FORCEXYBILLBOARD
	+ALLOWPARTICLES
    +CLIENTSIDEONLY
    States
    {
    Spawn:
		BOS2 I 3 bright A_SpawnItem("GreenFlare",0,0)
		TNT1 A 0 A_SpawnItemEx("PlasmaParticleSpawner", 0, 0, 0, 6, 6, 6, 0, 128)
		TNT1 A 0 A_Explode ( 5, 200 )
		TNT1 A 0 A_CustomMissile( "GreenCloudLarge", 12, 0, random( 0, 359 ), 2 )
		//TNT1 A 0 A_CustomMissile( "GreenCloudLarge", 12, 0, random( 0, 359 ), 2 )
	//	TNT1 A 0 A_CustomMissile( "GreenCloudMedium", 12, 0, random( 0, 359 ), 2 )
		TNT1 A 0 A_CustomMissile( "GreenCloudMedium", 12, 0, random( 0, 359 ), 2 )
		BOS2 J 3 bright A_SpawnItem("GreenFlare",0,0)
		TNT1 A 0 A_Explode ( 4, 195 )
		TNT1 A 0 A_CustomMissile( "GreenCloudLarge", 12, 0, random( 0, 359 ), 2 )
		TNT1 A 0 A_CustomMissile( "GreenCloudMedium", 12, 0, random( 0, 359 ), 2 )
		TNT1 A 0 A_CustomMissile( "GreenCloudMedium", 12, 0, random( 0, 359 ), 2 )
		TNT1 A 0 A_SpawnItem("GreenFlare",0,0) 
		BOS2 K 3 Bright A_SpawnItem("GreenFlare",0,0)
		TNT1 A 0 A_Explode ( 4, 190 )
		TNT1 A 0 A_CustomMissile( "GreenCloudLarge", 12, 0, random( 0, 359 ), 2 )
		TNT1 A 0 A_CustomMissile( "GreenCloudMedium", 12, 0, random( 0, 359 ), 2 )
		TNT1 A 0 A_CustomMissile( "GreenCloudMedium", 12, 0, random( 0, 359 ), 2 )
		TNT1 A 1 A_SpawnItem("GreenFlare",0,0)
		BOS2 L 3 Bright A_SpawnItem("GreenFlare",0,0)
		TNT1 A 0 A_Explode ( 4, 190 )
		TNT1 A 0 A_CustomMissile( "GreenCloudSmall", 12, 0, random( 0, 359 ), 2 )
		TNT1 A 0 A_CustomMissile( "GreenCloudMedium", 12, 0, random( 0, 359 ), 2 )
		TNT1 A 0 A_CustomMissile( "GreenCloudMedium", 12, 0, random( 0, 359 ), 2 )
		BOS2 M 3 A_SpawnItem("GreenFlare",0,0)
		TNT1 A 0 A_Explode ( 3, 180 )
		TNT1 A 0 A_CustomMissile( "GreenCloudMedium", 12, 0, random( 0, 359 ), 2 )
		TNT1 A 0 A_CustomMissile( "GreenCloudSmall", 12, 0, random( 0, 359 ), 2 )
		TNT1 A 0 A_CustomMissile( "GreenCloudSmall", 12, 0, random( 0, 359 ), 2 )
		BOS2 N 3 bright A_SpawnItem("GreenFlare",0,0)
		TNT1 A 0 A_Explode ( 3, 180 )
		TNT1 A 0 A_CustomMissile( "GreenCloudMedium", 12, 0, random( 0, 359 ), 2 )
		TNT1 A 0 A_CustomMissile( "GreenCloudSmall", 12, 0, random( 0, 359 ), 2 )
		TNT1 A 0 A_CustomMissile( "GreenCloudSmall", 12, 0, random( 0, 359 ), 2 )
		TNT1 A 0 A_CustomMissile( "GreenCloudMedium", 12, 0, random( 0, 359 ), 2 )
		BOS2 O 3 bright A_SpawnItem("GreenFlare",0,0)
        Goto Death
		
	Death:
		TNT1 A 0 A_SpawnItemEx("PlasmaParticleSpawner", 0, 0, 0, 6, 6, 6, 0, 128)
		Stop
    }
}




ACTOR BrownCloudSmall 
{
	PROJECTILE
	-ACTIVATEIMPACT
	-ACTIVATEPCROSS
	+NODAMAGETHRUST
	+ADDITIVEPOISONDAMAGE
	+BLOODLESSIMPACT
    +NOTELEPORT
	+RIPPER
	+NOEXTREMEDEATH
	-EXTREMEDEATH
	+FORCEXYBILLBOARD
	+ALLOWPARTICLES
	PoisonDamage 1
	Speed 1
	RenderStyle Translucent
	Alpha 0.6
	Seesound "redburn"
	States
	{
	Spawn:
		BTXS ABCDE 7
		BTXS A 0 A_Explode ( 10, 32 )
		Loop
	}
}

ACTOR BrownCloudMedium : BrownCloudSmall
{
	States
	{
	Spawn:
		BTXM ABCDE 7 A_SpawnItem("GreenFlare",0,0)
		BTXM A 0 A_Explode ( 15, 64 )
		Loop
	}
}

ACTOR BrownCloudLarge : BrownCloudSmall
{
	States
	{
	Spawn:
		BTXL A 0 A_Jump( 90, "SpawnNew" )
		BTXL ABCDE 7 A_SpawnItem("GreenFlare",0,0)
		BTXL A 0 A_Explode ( 20, 96 )
		Loop
	SpawnNew:
		TNT1 A 0 A_CustomMissile( "BrownCloudMissile", 12, 0, random( 0, 359 ), 2 )
		Goto Spawn+1
	}
}

ACTOR BrownCloudMissile
{
	
	PROJECTILE
	+SHOOTABLE
	+NOBLOOD
	+RIPPER
	+BLOODLESSIMPACT
	+NODAMAGETHRUST
	Speed 1
	Damage 0
	DamageType PoisonDamage
	RenderStyle Translucent
	Alpha 0.0
	States
	{
	Spawn:
		BTXS ABCDE 7 A_FadeIn( 0.12 )
		BTXS A 0 A_Explode ( 10, 24 )
		BTXS ABCDE 7 A_SpawnItem("GreenFlare",0,0)
		BTXS A 0 A_Explode ( 9, 20 )
		BTXS ABCDE 7 A_SpawnItem("GreenFlare",0,0)
		BTXS A 0 A_Explode ( 8, 16 )
		BTXS ABCDE 7 A_SpawnItem("GreenFlare",0,0)
		BTXS A 0 A_Explode ( 7, 12 )
		BTXS ABCDE 7 A_FadeOut( 0.06 )
		BTXS A 0 A_Explode ( 5, 8 )
		BTXS ABCDE 7 A_FadeOut( 0.06 )
		Stop
	Death:
		BTXS ABCDE 7 A_FadeOut( 0.06 )
		BTXS A 0 A_Explode ( 5, 8 )
		BTXS ABCDE 7 A_FadeOut( 0.06 )
		Stop
	}
}

